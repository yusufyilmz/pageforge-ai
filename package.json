{
  "name": "pageforge",
  "version": "1.0.0",
  "description": "Next.js + Supabase + Once UI starter template",
  "scripts": {
    "dev": "next dev --port 3001",
    "dev:turbo": "next dev --turbopack --port 3001",
    "export": "next export",
    "build": "next build",
    "start": "next start",
    "biome-write": "npx @biomejs/biome format --write .",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "supabase:gen-types": "supabase gen types typescript --local > src/lib/supabase/types.ts",
    "lint": "next lint --ignore-path .eslintignore",
    "lint:fix": "next lint --fix --ignore-path .eslintignore",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,css,scss,yml,yaml}\" --ignore-path .prettierignore",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,scss,yml,yaml}\" --ignore-path .prettierignore",
    "type-check": "tsc --noEmit --skipLibCheck",
    "generate-section": "tsx src/components/universal-section/generateSection.ts"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.8",
    "camelcase-keys": "^9.1.3",
    "classnames": "^2.5.1",
    "compressorjs": "^1.2.1",
    "date-fns": "^4.1.0",
    "next": "15.2.3",
    "openai": "^5.0.1",
    "postcss": "^8.4.39",
    "postcss-preset-env": "^9.5.15",
    "prismjs": "^1.30.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "^5.2.1",
    "react-masonry-css": "^1.0.16",
    "recharts": "^2.15.1",
    "sass": "^1.77.6",
    "sharp": "^0.33.4",
    "snakecase-keys": "^8.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@csstools/postcss-global-data": "^2.1.1",
    "@types/node": "20.17.23",
    "@types/prismjs": "^1.26.5",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.3.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "postcss-custom-media": "^10.0.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "^3.5.3",
    "tailwind-merge": "^3.3.0",
    "typescript": "5.8.2"
  },
  "overrides": {
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2"
  }
}
